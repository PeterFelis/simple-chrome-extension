{
  "name": "GS lazy vrimibo ",
  "version": "1.0",
  "description": "Nooit meer klikken",
  "manifest_version": 2,
  "icons": {
    "16": "icon/gs16.png",
    "32": "icon/gs32.png",
    "48": "icon/gs48.png",
    "128": "icon/gs128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["bookmarks","storage","declarativeContent"],
  "browser_action": {
    "default_icon": {
      "16": "icon/shuttle16.png",
      "32": "icon/shuttle32.png"
    },
    "default_title": "1e stap chrome extension",
    "default_popup": "popup.html"
  },
    "commands": {
      "toggle-badge": {
        "suggested_key": {
          "default": "Ctrl+Shift+B",
          "mac": "MacCtrl+Shift+B"
        },
        "description": "CTRL SHIF B to toggle status badge"
      }
    }
    }
    
